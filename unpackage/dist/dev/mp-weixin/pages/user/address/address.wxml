<view class="home"><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack="{{true}}" isCustom="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">配送地址</view></cu-custom><view class="center"><view class="tui-safe-area"><view class="tui-address"><block wx:for="{{addressList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><tui-list-cell vue-id="{{'2-'+index}}" padding="0" bind:__l="__l" vue-slots="{{['default']}}"><view data-event-opts="{{[['tap',[['editAddr',['$0'],[[['addressList','',index]]]]]]]}}" class="tui-address-flex" bindtap="__e"><view class="tui-address-left"><view class="tui-address-main"><view class="tui-address-name tui-ellipsis">{{item.userName}}</view><view class="tui-address-tel">{{item.userPhone}}</view></view><view class="tui-address-detail"><block wx:if="{{item.isDefault===1}}"><view class="tui-address-label">默认</view></block><text>{{item.areaIdName+item.userAddress}}</text></view></view><view class="tui-address-imgbox"><text class="cuIcon-write"></text></view></view></tui-list-cell></block></block></view><view class="tui-address-new text-center"><button data-event-opts="{{[['tap',[['editAddr',['$event']]]]]}}" class="cu-btn bg-blue lg" style="width:80%;" bindtap="__e">+新增配送地址</button></view></view></view></view>